#include<stdio.h>
int main()
{
    int n,i,c=0,a[10];
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        if(a[i]%2!=0)
        {
            c++;
            a[i]=1;
        }
    //printf("%d ",a[i]);
    }
    if(c!=0)
    {
       
       for(i=0;i<n;i++)
       {
           printf("%d ",a[i]);
       }
    }
    else
    {
         printf("Invalid input");
    }
}
