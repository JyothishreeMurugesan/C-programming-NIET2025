#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    if(n<0||n>10)
    {
        printf("Invalid input");
    }
    else if(n>=1&&n<=3)
    {
        printf("1\n");
        printf("1");
    }
    else if(n>=4 && n<=6)
    {
        printf("2\n");
        printf("1");
    }
    else if(n>=7 && n<=9)
    {
        printf("3\n");
        printf("2\n");
        printf("1");
    }
    else
    {
        printf("5\n");
        printf("3\n");
        printf("2\n");
        printf("1");
    }
}
