#include<stdio.h>
int main()
{
    int n,m,num,i,j;
    scanf("%d",&n);
    scanf("%d",&m);
    scanf("%d",&num);
    if((num>1)||(num<0))
    {
        printf("Invalid input");
    }
    else
    {
        for(i=0;i<n;i++)
        {
            for(j=0;j<m;j++)
            {
                printf("%d ",num);
            }
            printf("\n");
        }
    }
}
