#include<stdio.h>
int main()
{
    int n,a[10],i;
    scanf("%d",&n);
if(n<=0)
{
        printf("No readings provided");
        return 0;
}
else
{
    for(i=0;i<n;i++)
    {
    scanf("%d",&a[i]);
    }
    int sum=0;
    float avg;
    for(i=0;i<n;i++)
    {
        sum+=a[i];
    }
    avg=(float)sum/n;
    printf("%.2f",avg);
}
}
