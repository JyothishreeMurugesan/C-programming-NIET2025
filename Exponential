#include<stdio.h>
#include<math.h>

int exponential(int base, int expo)
{
    int result=pow(base,expo);
    printf("%d",result);
}
int main()
{
    int base,expo;
    scanf("%d",&base);
    scanf("%d",&expo);
    if(base>0)
    {
    exponential(base,expo);
    }
    else
    {
        printf("Invalid input");
    }
}
